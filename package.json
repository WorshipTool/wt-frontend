		"test:e2e:smoke:preview": "NEXT_PUBLIC_FRONTEND_URL=https://preview.chvalotce.cz NEXT_PUBLIC_BACKEND_URL=https://worshiptool-preview.fly.dev TEST_WITH_SLOWDOWN=false CI=true npm run test:e2e:smoke",
